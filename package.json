{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "biome lint ./src/",
    "lint:fix": "biome lint --apply ./src/",
    "format": "biome format --write ./src/",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "biome check --apply"
    ]
  }
}
